<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" fillWidth="false" prefHeight="200.0"
	prefWidth="320.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/17"
	fx:controller="modules.workshop.login.gui.LoginController">
	<children>
		<TextField fx:id="usernameText" promptText="Username" />
		<PasswordField fx:id="passwordText" promptText="Password" />
		<Button defaultButton="true" mnemonicParsing="false" onAction="#onLogin" text="Login" />
	</children>
</VBox>
